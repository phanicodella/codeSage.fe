{
  "name": "codesage",
  "version": "1.0.0",
  "description": "AI-powered offline code analysis tool",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "axios": "^1.6.7",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "framer-motion": "^10.18.0",
    "d3": "^7.8.5",
    "dagre-d3": "^0.6.4",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.330.0",
    "papaparse": "^5.4.1",
    "plotly.js-dist": "^2.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-plotly.js": "^2.6.0",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "socket.io-client": "^4.7.4",
    "web-vitals": "^2.1.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.56.0",
    "eslint-config-react-app": "^7.0.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.codesage.app",
    "productName": "CodeSage",
    "copyright": "Copyright Â© 2024 CodeSage",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development"
    },
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "publish": null
  }
}